{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "noEmit": true,
    "allowJs": true,
    "removeComments": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "/"
  },
  "include": ["src/**/*",
    "./test/setup"
  ],
}